# For testing and reference
# !Pre-load data into memory 0x00 and 0x01 before start!
# !Also set $s0 and $s1 to point 0x00 and 0x01!

INIT:
	lw		$t0, $s0
	lw		$t1, $s1
	j		MAIN
INTERNAL:
	addi	$t3, $t1, 512
	j		BEQTEST
MAIN:
	slti	$t2, $t1, 0
	jr		$t2
	j		INTERNAL
BEQTEST:
	beq		$t2, $t1, END
	bne		$t2, $t1, BNETEST
BNETEST:
	addi	$t2, $t2, 2
	j BEQTEST
END:
