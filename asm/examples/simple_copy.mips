# Simple copy and move value
# from one memory position
# to other memory position

addi	$s0, $zero, 0x00
addi	$s1, $zero, 0x01
addi	$s2, $zero, 0x02
	
addi	$s3, $zero, 0x06
addi	$s4, $zero, 0x05
addi	$s5, $zero, 0x04
	
lw	$t0, 0($s0)
lw	$t1, 0($s1)
lw	$t2, 0($s2)
	
sw	$t0, 0($s3)
sw	$t1, 0($s4)
sw	$t2, 0($s5)